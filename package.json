{
  "name": "nid2-cli",
  "version": "1.0.0",
  "description": "Raises the popularity score on your package through nefarious means.",
  "author": "otoneko.",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "dependencies": {
    "@inquirer/prompts": "^5.5.0",
    "date-fns": "2.28.0",
    "gaxios": "5.0.0",
    "ora": "5.4.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "jest": "^29.7.0",
    "jest-mock-console": "1.2.3",
    "nock": "13.2.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.0",
    "release-it": "^17.6.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node .",
    "test": "jest --coverage --detectOpenHandles",
    "format": "prettier . --write & eslint . --fix",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "dev:cli": "nodemon --watch 'src/**/*.ts' --exec ts-node src/cli.ts",
    "release": "release-it"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/(coverage|lib).*/"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "commitMode": "direct",
      "push": true,
      "branch": "release-candidate"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "bin": {
    "nid2": "lib/src/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**"
  ],
  "keywords": [
    "npm",
    "npmjs",
    "npms",
    "popularity",
    "increaser",
    "spammer",
    "download-simulator",
    "package-stats",
    "cli-tool",
    "typescript",
    "nid",
    "nid2",
    "npm-increaser-downloads"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otoneko1102/nid2-cli.git"
  },
  "bugs": {
    "url": "https://github.com/otoneko1102/nid2-cli/issues"
  },
  "homepage": "https://github.com/otoneko1102/nid2-cli#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
